# TurboShells Environment Configuration
# Copy this file to .env and update with your values

# Encryption Configuration (for data security)
# Generate strong random passwords for production use
TURBO_SHELLS_ENCRYPTION_PASSWORD=your_secure_encryption_password_here_change_this_in_production
TURBO_SHELLS_ENCRYPTION_SALT=your_secure_salt_here_change_this_in_production

# Game Configuration (optional)
TURBO_SHELLS_DEBUG=true
TURBO_SHELLS_LOG_LEVEL=INFO

# Development Settings (optional)
TURBO_SHELLS_DEV_MODE=true
TURBO_SHELLS_SAVE_INTERVAL=300
